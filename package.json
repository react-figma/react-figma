{
  "name": "react-figma",
  "version": "0.0.34",
  "description": "Render React components to Figma",
  "scripts": {
    "tsc": "tsc -p tsconfig.json --noEmit",
    "test": "jest",
    "prettify": "prettier \"src/**/*.{js,jsx,ts,tsx}\" \"examples/**/*.{js,jsx,ts,tsx}\" --ignore-path ./.prettierignore --write && git add . && git status",
    "prettier:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\" \"examples/**/*.{js,jsx,ts,tsx}\"",
    "build": "npm run build:clean && npm run build:lib",
    "build:clean": "rimraf dist",
    "build:lib": "cross-env BABEL_ENV=production tsc -p tsconfig.json",
    "gitbook:clean": "rm -rf _book/",
    "gitbook:start": "gitbook serve",
    "gitbook:build": "gitbook build",
    "now-build": "npm run gitbook:build"
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-babel",
    "setupFiles": [
      "<rootDir>/setup.jest.js"
    ]
  },
  "author": "Ilya Lesik <ilialesik@gmail.com>",
  "license": "MIT",
  "pre-commit": [
    "prettify",
    "tsc",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/react-figma/react-figma.git"
  },
  "keywords": [
    "figma",
    "react",
    "figma-plugins"
  ],
  "engines": {
    "node": ">=10.15.0 <12.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.8",
    "@types/react": "^16.9.4",
    "@types/react-test-renderer": "^16.9.1",
    "babel-jest": "^24.9.0",
    "copyfiles": "^2.1.1",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "figma-api-stub": "0.0.27",
    "gitbook-cli": "^2.3.2",
    "gitbook-plugin-anchorjs": "^2.1.0",
    "gitbook-plugin-edit-link": "^2.0.2",
    "gitbook-plugin-github": "^2.0.0",
    "gitbook-plugin-prism": "^2.4.0",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.18.2",
    "react": "16.8.6",
    "react-figma-webpack-config": "0.0.2",
    "react-test-renderer": "16.8.6",
    "rimraf": "^3.0.0",
    "style-loader": "^1.0.0",
    "svg-inline-loader": "^0.8.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "yoga-layout-prebuilt": "1.9.3"
  },
  "dependencies": {
    "color-parse": "^1.3.8",
    "nanoid": "^2.0.3",
    "react-reconciler": "^0.23.0",
    "rxjs": "^6.5.2"
  },
  "peerDependencies": {
    "react": "*"
  }
}
