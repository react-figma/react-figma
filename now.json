{
  "version": 2,
  "name": "react-figma",
  "public": true,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "_book"
      }
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "routes": [
    {
      "src": "^/(.*)",
      "dest": "/$1"
    }
  ]
}